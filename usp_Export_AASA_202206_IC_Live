	select distinct
		a1.studentKey, /* studentKey */
		a1.studentNumber, /* studentNumber */
		a1.personID, /* personID */
		a1.locationKey,
		a1.schoolID, /* schoolID */
		a1.[Test Code], /* testCode */
		a1.[TEST DATE (MM/DD/YYYY)], /* testDate */
