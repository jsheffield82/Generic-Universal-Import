/* Move on When reading */
	insert into #aasaVert
		(studentKey, studentNumber, personID, locationKey, schoolID, testCode, testDate, gradeLevel, 
		testName, testSubject, subtestName, rawScore, scaleScore, performance, status)
	select distinct
